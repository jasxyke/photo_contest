<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head', {title: 'Xylem | Home'});%>
</head>

<body>
    <!--NAVIGATION SECTION-->
   <%- include('../partials/navbar', {signedIn: signedIn});%> 

    <!--FEATURED CONTENT SECTION-->
   <header>
        <div class="head-left">
            <h3>| Featured Shots</h3>
        </div>
        <div class="head-right">
            <a href="/photo-entries">View More<span class="material-symbols-outlined">arrow_forward</span></a>           
        </div>
   </header>
   <div class="featured-con">
    <!--CARD SECTION-->
    <% if(entries){ %>
        <% entries.forEach((entry, index) => { %>
            <%- include('../partials/featured-card', {entry: entry, rank: index + 1}); %>
    <%    }); }%>  
   </div>
   <%- include('../partials/footer'); %> 
   <%- include('../partials/react-loader'); %>

   <% if(user){ %>
    <%- include('../partials/btn-script'); %>
    <% } %>
   
</body>
</html>